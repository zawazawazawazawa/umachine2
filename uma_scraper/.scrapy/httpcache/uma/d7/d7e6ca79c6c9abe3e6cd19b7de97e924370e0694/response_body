<script type="text/javascript">
$(document).ready(function() {
show_user();
});
function show_user(){
var is_stage = '';
var is_user  = '';
var is_alert = '';
var is_sp    = '';
var nickname = '';
var user_img = '';
if(is_sp) {
$('.sp_nk_btn').show();
}
if(! is_stage) {
$('.header_stage_area').remove();
}
if(is_user){
//ログイン中
$('.login_show').show();
if(is_alert) {
$('.Alert_New').show();
}
} else {
//未ログイン
$('.no_login_show').show();
$('.login_show').remove();
}
}
</script>

